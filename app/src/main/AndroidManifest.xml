<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="fathi.reza.been_tech">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".Utills.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.BeenTech"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".CheckOut.CheckOut">
            <intent-filter >
                <action android:name="android.intent.action.VIEW"/>
                <category android:name="android.intent.category.BROWSABLE"/>
                <category android:name="android.intent.category.DEFAULT"/>
                <data android:scheme="clicksite"/>
            </intent-filter>
        </activity>

        <activity
            android:name=".LoginAndRegister.RegidterActivity"
            android:exported="false" />
        <activity
            android:name=".LoginAndRegister.LoginActivity"
            android:exported="false" />
        <activity
            android:name=".Details.Compare.CompareActivity"
            android:exported="false" />
        <activity
            android:name=".Details.Compare.CompareListActivity"
            android:exported="false" />
        <activity
            android:name=".Details.Chart.PriceHistoryActivity"
            android:exported="false" />
        <activity
            android:name=".Details.PropertyActivity"
            android:exported="false" />
        <activity
            android:name=".Details.DetailsActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>